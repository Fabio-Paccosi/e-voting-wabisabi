# .env
NODE_ENV=production
DB_HOST=localhost
DB_PORT=5432
DB_NAME=evoting_wabisabi
DB_USER=postgres
DB_PASSWORD=your_secure_password

# Bitcoin Configuration
BITCOIN_NETWORK=testnet
BITCOIN_RPC_MAINNET=http://localhost:8332
BITCOIN_RPC_TESTNET=http://localhost:18332
BITCOIN_RPC_USER=bitcoinrpc
BITCOIN_RPC_PASSWORD=your_rpc_password

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key_for_private_keys

# CoinJoin
ENABLE_COINJOIN_TRIGGER=true
COINJOIN_CHECK_INTERVAL=30000

# Email Service
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password